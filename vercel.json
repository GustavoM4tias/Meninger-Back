{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ],
  "functions": {
    "server.js": {
      "includeFiles": ["node_modules/mysql2/**/*"]
    }
  }
}